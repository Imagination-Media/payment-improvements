<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/paymentrequestapi/shippingoptionslist" method="GET">
        <service class="Magento\PaymentRequestApi\Api\ShippingOptionsListInterface" method="get"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/paymentrequestapi/requestMerchantSession" method="GET">
        <service class="Magento\PaymentRequestApi\Api\Data\RequestMerchantSessionInterface" method="get"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>